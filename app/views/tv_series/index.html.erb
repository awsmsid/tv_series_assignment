<div class="container-fluid datatable-container">
  <div class="row">
    <div class="col-md-12">
      <table id="tvSeriesDatatable" class="table table-striped table-bordered" data-source="<%= tv_series_index_path(format: :json) %>">
        <thead>
          <tr>
            <th>Name</th>
            <th>Number of Seasons</th>
            <th>Date of First Release</th>
            <th>Genre</th>
            <th>Actor</th>
            <th>Director</th>
            <th>Shoot Locations</th>
            <th>Country</th>
            <th>Rating</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>